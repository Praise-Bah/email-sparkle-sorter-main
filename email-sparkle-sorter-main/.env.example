# .env.example
# Copy this file to .env and fill in your own values.

# Google OAuth2 credentials
VITE_GOOGLE_CLIENT_ID=
VITE_GOOGLE_CLIENT_SECRET=
VITE_GOOGLE_API_KEY=
VITE_REDIRECT_URI=http://localhost:5173/auth/callback

# OpenAI
VITE_OPENAI_API_KEY=
# VITE_OPENAI_MODEL= (optional, default 'openai/o4-mini-high')
# VITE_OPENAI_API_URL= (optional, default 'https://openrouter.ai/api/v1/chat/completions')

# Deepseek.ai
VITE_DEEPSEEK_API_KEY=
# Optional Deepseek.ai model (default 'deepseek-chat')